{
  "name": "inertia-page-loader",
  "version": "0.6.0",
  "packageManager": "pnpm@7.1.1",
  "description": "The plugin page loader for Inertia.js",
  "author": "Lucas Yang <yangchenshin77@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ycs77/inertia-page-loader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/inertia-page-loader.git"
  },
  "bugs": {
    "url": "https://github.com/ycs77/inertia-page-loader/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.mjs"
    },
    "./runtime": {
      "require": "./dist/runtime.cjs",
      "import": "./dist/runtime.mjs"
    },
    "./types": {
      "require": "./dist/types.cjs",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "unpkg": "dist/runtime.iife.js",
  "jsdelivr": "dist/runtime.iife.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
    "build:plugin": "tsup",
    "build:runtime": "tsup --config tsup.runtime-config.ts",
    "build:fix": "tsx scripts/postbuild.ts",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit \"Release v%s\" && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "fast-glob": "^3.2.12",
    "unplugin": "^1.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.11.18",
    "@ycs77/eslint-config": "^0.1.3",
    "bumpp": "^8.0.0",
    "eslint": "^8.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.10.0",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "typescript": "~4.7.4",
    "vite": "^4.4.0",
    "vitest": "^0.27.0",
    "webpack": "^5.76.0"
  }
}
